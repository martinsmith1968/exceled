{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information",
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleThemes::Code, Serilog.Sinks.Console",
          "zztheme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleThemes::Literate, Serilog.Sinks.Console",
          "outputTemplate": "{Timestamp:T} [{Level:u3}] {CommandName}: {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%\\ExcelEditor-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({MachineName}/{ProcessId}) {SourceContext}: {Message:j}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "WithMachineName",
      "WithThreadId",
      "WithProcessId"
    ],
    "Properties": {
      "Application": "Excel Editor"
    }
  }
}
